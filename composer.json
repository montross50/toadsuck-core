{
	"name": "toadsuck/core",
	"description": "Front controller and other core libraries used by the Toadsuck project.",
	"homepage": "https://github.com/toadsuck/toadsuck-core",
	"license": "MIT",
	"authors": [
		{
			"name": "Josh Moody",
			"email": "jgmoody@gmail.com"
		}
	],
	"autoload":{
		"psr-4":{
			"Toadsuck\\Core\\Tests\\": "tests/",
			"Toadsuck\\Core\\Tests\\App\\": "tests/resources/",
			"Toadsuck\\Core\\Tests\\App\\Controllers\\": "tests/resources/controllers/",
			"Toadsuck\\Core\\Tests\\App\\Models\\": "tests/resources/models/",
			"Toadsuck\\Core\\": "src/"
		}
	},

	"require": {
		"php": ">= 5.4",
		"aura/router": "2.*@dev",
		"league/plates": "1.0.*",
		"fuelphp/config": "dev-master",
		"illuminate/database": "4.2.*@dev",
		"prelude/prelude-dsn": "v1.0",
		"symfony/http-foundation": "2.5.*@dev"
	},

	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"squizlabs/php_codesniffer": "1.*"
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/montross50/prelude-dsn"
		}
    	],
	"minimum-stability": "dev"
}
